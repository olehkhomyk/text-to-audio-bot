    "elevenlabs": "1.59.0",
    "express": "^5.1.0",
    "lodash": "~4.17.21",
    "node-fetch": "^2.7.0",
    "rxjs": "~7.8.1",
    "telegraf": "~4.16.3"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/express": "^5.0.5",
    "@types/lodash": "~4.17.14",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.12",
    "dotenv-cli": "^8.0.0",
    "nodemon": "~3.1.9",
