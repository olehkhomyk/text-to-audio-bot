{
  "name": "solana_node",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:local": "dotenv -e .env node dist/index.js",
    "start:dev": "dotenv -e .env -- nodemon src --ext ts --exec \"npx ts-node\" src/index.ts",
    "start:dev:watch": "dotenv -e .env -- nodemon --watch src --ext ts --exec \"npx ts-node\" src/index.ts"
  },
